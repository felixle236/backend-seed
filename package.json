    "readmeFilename": "README.md",
    "scripts": {
        "eslint": "./node_modules/.bin/eslint --ext .ts --ext .js ./src",
        "test": "npm run eslint && NODE_ENV=development ./node_modules/.bin/nyc ./node_modules/.bin/mocha --exit --require ts-node/register ./src/test/**/*.ts",
        "build": "npm run eslint && ./node_modules/.bin/tsc && ./node_modules/.bin/grunt sync",
        "start": "./node_modules/.bin/nodemon --watch 'src/**/*.ts' --exec 'NODE_ENV=development ./node_modules/.bin/ts-node' src/app.ts",
        "staging": "NODE_ENV=staging node ./dest/app.js",
        "production": "NODE_ENV=production node ./dest/app.js"
    },
