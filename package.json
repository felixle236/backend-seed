{
    "name": "bankend-seed",
    "version": "2.0.0",
    "description": "Backend environment",
    "scripts": {
        "watch": "./node_modules/.bin/nodemon --watch 'src/**/*.ts' --exec 'NODE_ENV=Development SINGLE_THREAD=true ./node_modules/.bin/ts-node' src/server.ts",
        "eslint": "./node_modules/.bin/eslint --ext .ts --ext .js ./src",
        "test": "NODE_ENV=Development ./node_modules/.bin/nyc ./node_modules/.bin/mocha --exit ./src/test/**/*.ts",
        "build": "npm run eslint && ./node_modules/.bin/grunt sync && ./node_modules/.bin/tsc",
        "start": "NODE_ENV=Development SINGLE_THREAD=true ./node_modules/.bin/ts-node ./src/server.ts",
        "staging": "NODE_ENV=Staging node ./dest/server.js",
        "production": "NODE_ENV=Production node ./dest/server.js"
    },
    "dependencies": {
        "cron": "^1.3.0",
        "email-existence": "^0.1.6",
        "express": "^4.16.3",
        "mongoose": "^5.1.1",
        "morgan": "^1.9.0",
        "nedb": "^1.8.0",
        "nodemailer": "^4.6.4",
        "request": "^2.86.0",
        "request-promise": "^4.2.2",
        "validator": "^10.2.0"
    },
    "devDependencies": {
        "@types/chai": "^4.1.3",
        "@types/cron": "^1.3.0",
        "@types/express": "^4.11.1",
        "@types/mocha": "^5.2.0",
        "@types/mongoose": "^5.0.14",
        "@types/morgan": "^1.7.35",
        "@types/nedb": "^1.8.5",
        "@types/node": "^10.1.0",
        "@types/nodemailer": "^4.6.0",
        "chai": "^4.1.2",
        "eslint": "^4.19.1",
        "eslint-config-google": "^0.9.1",
        "grunt": "^1.0.2",
        "grunt-exec": "^3.0.0",
        "grunt-sync": "^0.7.0",
        "mocha": "^5.1.1",
        "nodemon": "^1.17.4",
        "nyc": "^11.8.0",
        "ts-node": "^6.0.3",
        "typescript": "^2.8.3",
        "typescript-eslint-parser": "^15.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/felixle236/backend-seed.git"
    },
    "keywords": [
        "backend-seed",
        "nodejs",
        "typescript",
        "mongodb",
        "mongoose",
        "multi-layer",
        "singleton"
    ],
    "author": {
        "name": "Felix Le",
        "email": "felix.le.236@gmail.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/felixle236/backend-seed/issues"
    },
    "homepage": "https://github.com/felixle236/backend-seed#readme"
}
